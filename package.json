    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^8.20.0",
    "@typescript-eslint/parser": "^8.20.0",
    "eslint": "^9.18.0",
    "globals": "^15.14.0",
    "jest": "^29.7.0",
    "mongodb-memory-server": "^10.1.3",
    "prettier": "^3.4.2",
    "supertest": "^7.0.0",
